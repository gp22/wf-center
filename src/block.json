{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wazframe/wf-center",
	"version": "0.1.0",
	"title": "Center",
	"category": "design",
	"icon": "align-center",
	"description": "Every Layout components.",
	"supports": {
		"html": false
	},
	"textdomain": "wf-center",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"size": {
			"type": "string",
			"default": "small"
		},
		"intrinsic": {
			"type": "boolean",
			"default": false
		},
		"andText": {
			"type": "boolean",
			"default": false
		}
	}
}
